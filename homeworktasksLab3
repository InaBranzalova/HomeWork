#  Option 1


print(f'|{"="*10}|{"="*10}|')
print(f'|{1:>10}|{1:<10}|')
print(f'|{23:>10}|{23:<10}|')
print(f'|{456:>10}|{456:<10}|')
print(f'|{"="*10}|{"="*10}|')

# Option 2

print("|","|","|",sep="="*10)
print("|","1|1","|",sep=' '*9)
print("|","23|23","|",sep=' '*8)
print("|","456|456","|",sep=' '*7)
print("|","|","|",sep="="*10)